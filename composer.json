{
    "name": "adrhumphreys/trello-workflow",
    "description": "Workflows in elemental with Trello",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4",
        "dnadesign/silverstripe-elemental": "^4.5",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "AdrHumphreys\\Workflow\\": "src/",
            "AdrHumphreys\\Workflow\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
